# define in ~/.config/spark/.env

# Vercel AI Gateway (routes to any provider)
AI_GATEWAY_API_KEY=your_gateway_api_key_here

# Or use individual provider keys (for direct mode / fallback)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Upstash Vector for semantic search
UPSTASH_VECTOR_REST_URL=https://your-index.upstash.io
UPSTASH_VECTOR_REST_TOKEN=your-token